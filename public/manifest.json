{
  "manifest_version": 3,
  "name": "Fun extension",
  "description": "Your custom YouTube Playlist",
  "version": "1.0.0",
  "icons": {
    "256": "logo512.png"
  },
  "action": {
    "default_icon": "logo512.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}